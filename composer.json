{
    "name": "lizhineng/laravel-mns-queue-driver",
    "description": "Laravel queue driver for MNS.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Li Zhineng",
            "email": "im@zhineng.li"
        }
    ],
    "require": {
        "illuminate/queue": "^8",
        "illuminate/contracts": "^8",
        "aliyun/aliyun-mns-php-sdk": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Zhineng\\QueueMns\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zhineng\\QueueMns\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zhineng\\QueueMns\\MnsServiceProvider"
            ]
        }
    }
}
